{
  "name": "tourze/symfony-aop-bundle",
  "description": "AOP for Symfony Framework",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "friendsofphp/proxy-manager-lts": "^1.0.18",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/console": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/expression-language": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/stopwatch": "^6.4",
    "symfony/service-contracts": "^3",
    "tourze/arrayable": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\Aop\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\Aop\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1",
    "maglnet/composer-require-checker": "^4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}