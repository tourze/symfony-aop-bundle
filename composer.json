{
  "name": "tourze/symfony-aop-bundle",
  "description": "AOP for Symfony Framework",
  "license": "MIT",
  "require": {
    "friendsofphp/proxy-manager-lts": "^1.0.18",
    "psr/cache": "^2.0 || ^3.0",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/expression-language": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/stopwatch": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/arrayable": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/symfony-dependency-service-loader": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\Aop\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\Aop\\Tests\\": "tests"
    }
  }
}